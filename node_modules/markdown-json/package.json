{
  "name": "markdown-json",
  "version": "1.4.0",
  "description": "Markdown to json has yaml support and converts your markdown files to json so you can use them as a static api.",
  "main": "markdown-json.js",
  "bin": {
    "markdown-json": "markdown-json.js"
  },
  "scripts": {
    "test": "test",
    "release": "semantic-release"
  },
  "author": "Klayton Faria <klayton.souza@gmail.com> (http://klayton.io)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/klaytonfaria/markdown-json/issues"
  },
  "homepage": "https://github.com/klaytonfaria/markdown-json#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/klaytonfaria/markdown-json.git"
  },
  "keywords": [
    "markdown",
    "json",
    "parser",
    "yaml",
    "html"
  ],
  "devDependencies": {
    "@semantic-release/changelog": "^3.0.1",
    "@semantic-release/commit-analyzer": "^6.1.0",
    "@semantic-release/git": "^7.0.5",
    "@semantic-release/release-notes-generator": "^7.1.4",
    "eslint": "^5.9.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-watch": "^4.0.2",
    "semantic-release": "^15.12.0"
  },
  "dependencies": {
    "cli": "^1.0.1",
    "jsonfile": "^5.0.0",
    "metalsmith": "^2.2.0",
    "metalsmith-collections": "^0.9.0",
    "metalsmith-excerpts": "^1.1.0",
    "metalsmith-filter": "^1.0.2",
    "metalsmith-ignore": "^1.0.0",
    "metalsmith-markdown": "^1.1.0",
    "metalsmith-prompt": "0.0.8",
    "metalsmith-to-json": "1.0.0",
    "metalsmith-updated": "^0.1.4"
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "docs/CHANGELOG.md"
        }
      ],
      "@semantic-release/git"
    ]
  }
}
